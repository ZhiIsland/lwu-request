{
  "name": "lwu-request",
  "version": "1.0.0",
  "description": "基于uniapp网络请求API封装的轻量级网络请求库，支持自动刷新token，请求拦截，请求中断，自动重试等特性",
  "scripts": {
    "build:uni": "cd packages/uniapp-request && tsup --config ./tsup.config.ts",
    "publish": "cd packages/uniapp-request && npm publish",
    "docs:dev": "cd docs && npm run docs:dev",
    "docs:build": "cd docs && npm run docs:build",
    "docs:preview": "cd docs && npm run docs:preview"
  },
  "author": "kviewui@163.com",
  "license": "MIT",
  "dependencies": {
    "tsup": "^6.7.0",
    "typescript": "^5.0.2"
  },
  "private": "true",
  "devDependencies": {
    "@dcloudio/types": "^3.3.3",
    "@types/axios": "^0.14.0",
    "@types/node": "^20.2.3"
  }
}
